{
  "buildCommand": "npm run build",
  "outputDirectory": "dist/spa",
  "devCommand": "npm run dev",
  "installCommand": "npm install",
  "framework": null,
  "rewrites": [
    {
      "source": "/api/:match*",
      "destination": "http://api.qamanager.online:3000/api/:match*"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}

