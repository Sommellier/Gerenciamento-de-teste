{
  "extends": "./.quasar/tsconfig.json",
  "compilerOptions": {
    "strict": true,
    "skipLibCheck": true,
    "downlevelIteration": true,
    "moduleResolution": "bundler",
    "allowImportingTsExtensions": false,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "target": "ES2022",
    "lib": ["ES2022", "DOM", "DOM.Iterable"]
  },
  "exclude": [
    "node_modules",
    "dist",
    "testes",
    "tests",
    "**/*.test.ts",
    "**/*.test.tsx",
    "**/*.spec.ts",
    "**/*.spec.tsx",
    "vitest.config.ts",
    "tsconfig.test.json"
  ]
}